@{
    Layout = null;
}
@model ActivityClubPortal.API.Resources.MemberResource
<!DOCTYPE html>
<html lang="en">

<head>
    <meta charset="utf-8">
    <meta name="viewport" content="width=device-width, initial-scale=1.0">
    <script src="https://code.jquery.com/jquery-3.5.1.min.js"></script>
    <link rel="stylesheet" href="https://cdnjs.cloudflare.com/ajax/libs/font-awesome/6.0.0-beta3/css/all.min.css">
    <meta name="author" content="templatemo">
    <meta name="viewport" content="width=device-width, initial-scale=1, shrink-to-fit=no">

    <link href="https://fonts.googleapis.com/css2?family=Roboto:wght@100;300;400;500;700;900&display=swap" rel="stylesheet">

    <title>SnapX Photography by TemplateMo</title>
    <script src="https://maxcdn.bootstrapcdn.com/bootstrap/4.5.2/js/bootstrap.min.js"></script>
    <link rel="stylesheet" href="~/Register.css" asp-append-version="true" />

    <style>
  
    </style>
</head>

<body>

    <div class="container">
        <h2 id="login_title">Sign Up </i></h2>
        <form id="signInForm" method="post">
            <input asp-for="Id" value="0" hidden/>
            <i class="fas fa-user"></i><label for="email">FullName</label>
            <input asp-for="FullName" type="text" id="FullName" placeholder="Enter your FullName" required>

            <i class="fas fa-envelope"></i><label for="email">Email</label>
            <input asp-for="Email" type="text" id="email" placeholder="Enter your email" required>
            <small id="emailAvailability" class="form-text text-danger"></small>

            <i class="fas fa-lock"></i>
            <label for="password">Password</label>
            <input asp-for="Password" type="password" class="form-control" id="password" placeholder="Enter a password" required>
            <span id="password-requirements" class="text-danger"></span>

            <label for="DateOfBirth">DOB</label> <br />
            <input asp-for="DateOfBirth" type="date" id="DateOfBirth" required>
           <br />
            <label for="MobileNumber">Mobile Number</label>
            <input asp-for="MobileNumber" type="text" id="MobileNumber" placeholder="Enter your Mobile Number" required>

            <label for="EmergencyNumber">Emergency Number</label>
            <input asp-for="EmergencyNumber" type="text" id="EmergencyNumber" placeholder="Enter your Emergency Number" required>

            <label for="Profession">Profession</label>
            <input asp-for="Profession" type="text" id="Profession" placeholder="Enter your Profession" required>

            <label for="Nationality">Nationality</label>
            <input asp-for="Nationality" type="text" id="Nationality" placeholder="Enter your Nationality" required>

            <button type="submit" class="btn btn-info">Sign In <i class="fas fa-arrow-right" id="login_icon"></i></button>
        </form>
    </div>

    <script src="https://ajax.googleapis.com/ajax/libs/jquery/3.5.1/jquery.min.js"></script>
    <script src="https://maxcdn.bootstrapcdn.com/bootstrap/4.5.2/js/bootstrap.min.js"></script>
    <!-- Scripts -->
    <!-- Bootstrap core JavaScript -->


</body>
</html>
